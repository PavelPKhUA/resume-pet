<main>
    {{#with heading}}
    <section>
        {{#with main}}
        <div>
            <h1>{{title}}</h1>
            <p>{{text}}</p>
            {{#with button_text}}
            <button class="btn btn-primary">{{solid}}</button>
            <br><br><br>
            <button class="btn btn-outline-primary">{{outline}}</button>
            {{/with}}
        </div>
        {{/with}}
            
        <br><br><br>

        {{#with second}}
        <div>
            <h2>{{title}}</h2>
            <p>{{text}}</p>
            {{#with button_text}}
            <button class="btn btn-secondary">{{solid}}</button>
            <br><br>
            <button class="btn btn-outline-secondary">{{outline}}</button>
            {{/with}}
        </div>
        {{/with}}

        <br><br><br>

        {{#with subblock}}
        <div>
            <h3>{{title}}</h3>
            <p>{{text}}</p>
            {{#with button_text}}
            <button class="btn btn-dark">{{solid}}</button>
            <br><br>
            <button class="btn btn-outline-dark">{{outline}}</button>
            {{/with}}
        </div>
        {{/with}}

    </section>
    {{/with}}

    <br><br><br>

    {{#with nested_display}}
    <section>
        <div class="card border-light">
          <div class="display-1">{{title}}</div>
            {{#with children_display}}
            <div class="card-body">
                <div class="display-4">{{title}}</div>
                <p>{{text}}</p>
                {{#with button}}
                <button class="btn btn-outline-warning">{{text}}</button>
                {{/with }}
            </div>
            {{/with}}  
        </div>
        
        <h6>{{header_text}}</h6>
        {{#with button}}
            <button class="btn btn-info">{{text}}</button>
        {{/with}}


    </section> 
    {{/with}}

    <br><br><br>

    {{#with paragraph}}
    <section>
        <p class="text-uppercase">{{title}}</p>
        <p class="text-lowercase">{{text}}</p>
        {{#with button}}
        {{>task21}}
        {{/with}}
        <br><br>
        {{#each list_name}}
        <ul>
            <li>
                {{>task21}}
            </li>
        </ul>            
        {{/each}}
    </section>
    {{/with}}

    
</main>